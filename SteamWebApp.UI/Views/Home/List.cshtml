@model GameListVM
@{
    ViewData["Title"] = "List";
}


<div class="row">
    <div class="col-md-3">
        @await Component.InvokeAsync("CategoryList")
    </div>

    <div class="col-md-9">

        <div class="row">
            @foreach (var item in Model.Games)
            {
                <partial name="_Game" for="@item" />
            }
        </div>
        <div page-model="@Model.PageInfo"></div>
    </div>
</div>
